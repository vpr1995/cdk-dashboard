{
  "name": "cdk-dashboard",
  "version": "0.1.0",
  "description": "AWS CDK construct for creating dashboards from CDK stacks",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepack": "npm run build",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "dashboard",
    "cloudwatch",
    "monitoring"
  ],
  "author": "vprprudhvi@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vpr1995/cdk-dashboard.git"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "aws-cdk-lib": "^2.190.0",
    "constructs": "^10.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ]
}
